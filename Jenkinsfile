pipeline{
   agent any
      stages{
        stage('clone ){
        steps{ 
            git branch:'main'.url:'https://github.com/kavalisravani13/calculator.git';
}
}
stgae('compile'){
steps{
 sh 'javac calculator.java'
}
}
stage('build'){
steps{py
sh 'javac calculator 25 5'
}
}
stgae('test'){
steps{
 sh 'javac calculator 30 -5'
}
}
stage('Deploy'){
steps{
echo 'Deployment completed'
}
}
}
}
